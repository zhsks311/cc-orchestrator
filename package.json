{
  "name": "cc-orchestrator-server",
  "version": "0.2.6",
  "description": "CC Orchestrator - MCP Server for multi-model agent orchestration in Claude Code",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "setup": "node scripts/setup.mjs",
    "update": "node scripts/update.mjs",
    "uninstall": "node scripts/uninstall.mjs",
    "publish": "node scripts/publish.mjs",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write src tests",
    "format:check": "prettier --check src tests",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "mcp",
    "claude-code",
    "multi-model",
    "orchestration",
    "llm",
    "agent"
  ],
  "author": "zhsks311",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhsks311/cc-orchestrator.git"
  },
  "homepage": "https://github.com/zhsks311/cc-orchestrator#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@ast-grep/napi": "^0.31.0",
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cc-orchestrator": "^0.2.2",
    "dotenv": "^17.2.3",
    "openai": "^4.77.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "prettier": "^3.8.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
