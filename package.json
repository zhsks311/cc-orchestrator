{
  "name": "cc-orchestrator",
  "version": "1.2.0",
  "description": "CC Orchestrator - MCP Server for multi-model agent orchestration in Claude Code",
  "main": "dist/index.js",
  "bin": {
    "cc-orchestrator": "./cli.mjs"
  },
  "type": "module",
  "scripts": {
    "setup": "node scripts/setup.mjs",
    "update": "node scripts/update.mjs",
    "uninstall": "node scripts/uninstall.mjs",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "mcp",
    "claude-code",
    "multi-model",
    "orchestration",
    "llm",
    "agent"
  ],
  "author": "zhsks311",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhsks311/cc-orchestrator.git"
  },
  "homepage": "https://github.com/zhsks311/cc-orchestrator#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "dotenv": "^17.2.3",
    "openai": "^4.77.0",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
